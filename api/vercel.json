{
    "name": "vercel-typescript-rest-api",
    "version": 2,
    "public": true,
    "installCommand": "npx prisma generate",
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/server.ts"
      }
    ]
  }